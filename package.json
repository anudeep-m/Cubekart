{
  "name": "cube-kart",
  "version": "1.0.0",
  "description": "Shopping App for Goggles",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
